/* pages/Tabars/Mine/Pages/OrderList/index.wxss */
.address_box_right{
  width: 20rpx;
  height: 36rpx;
  padding-right: 20rpx;
}
.order_price{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 20rpx;
  width: calc(100% - 40rpx);
}
.Produce_count{
  color: #9E9E9E;
}
.Produce_name{
  color: #EDEDED;
  flex-wrap: wrap;
}
.Produce_detai_box{
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  flex-direction: column;
  min-height: 170rpx;
  width: 100%;
  font-size: 32rpx;
  font-weight: 500;
}
.Procduce_left_chid_box{
  min-height: 170rpx;
  width: calc(100% - 170rpx);
  margin-left: 10rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.produce_image{
  width: 190rpx;
  height: 170rpx;
  margin-left: 20rpx;
}
.Procduce_box_left{
  display: flex;
  justify-content: space-between;
  align-items: center;
  min-height: 220rpx;
  margin-top: 10rpx;
}
.Procduce_box{
  min-height: 332rpx;
  background-color: #2B2B2B;
  margin-top: 10rpx;
  border-radius: 24rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #9E9E9E;
  
}
.time_box{
  width: 100%;
  color: #9E9E9E;
  height: 40rpx;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}
.order_list_item{
  min-height: 400rpx;
  width: calc(100% - 80rpx);
  /* background-color: #201F1E; */
  margin-left: 40rpx;
}
.order_list_item_box{
  background-color: #201F1E;
  min-height: 350rpx;
  width: 100%;
  border-radius: 20rpx;
}